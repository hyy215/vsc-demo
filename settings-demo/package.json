{
    "name": "settings-demo",
    "displayName": "settings-demo",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "settings-demo.helloWorld",
                "title": "Hello World"
            }
        ],
        "configuration": [
            {
                "id": "demo-1",
                "title": "demo 1",
                "order": 1,
                "properties": {
                    "demo1.test.enable.test1": {
                        "type": "boolean",
                        "default": true
                    },
                    "demo1.test.format.test1": {
                        "type": "boolean",
                        "default": true
                    }
                }
            },
            {
                "id": "demo-2",
                "title": "demo 2",
                "order": 2,
                "properties": {
                    "demo2.test1.enable1.test1": {
                        "type": "boolean",
                        "default": true
                    },
                    "demo2.test1.format1.test1": {
                        "type": "boolean",
                        "default": true
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.69.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.16.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "typescript": "^4.7.2",
        "ts-loader": "^9.3.0",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2",
        "@vscode/test-electron": "^2.1.3"
    }
}
